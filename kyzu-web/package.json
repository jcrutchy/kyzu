{
  "name": "kyzu-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:watch-wasm\" \"vite\"",
    "watch-wasm": "cd ../kyzu-core && cargo watch -i pkg/ -s \"wasm-pack build --target web --out-dir pkg\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  }
}
